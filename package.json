{
  "name": "@aori/aori-ts",
  "version": "0.0.16",
  "description": "Aori TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\"",
    "lint": "echo \"No linting configured\"",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "prerelease": "npm version prerelease --preid=beta",
    "release": "npm publish",
    "version:custom": "node scripts/version.js",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aori-io/aori-ts.git"
  },
  "keywords": [
    "aori",
    "ethereum",
    "web3",
    "sdk",
    "trading"
  ],
  "author": "Aori",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aori-io/aori-ts/issues"
  },
  "homepage": "https://github.com/aori-io/aori-ts#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
